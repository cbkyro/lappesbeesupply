{"components/product/quick-shop-modal":"<a href=\"#\" class=\"button-modal-close modal-close\">{{> svg/modal-close}}</a>\n\n<div class=\"modal-body\"\n  data-product-container\n  data-product-details\n  data-product-title=\"{{product.title}}\">\n\n  <div class=\"quick-shop-image-column\">\n    {{! Product Image}}\n    {{#if product.images}}\n      <div class=\"quick-shop-primary-image\" data-quickshop-images-primary>\n        {{#each product.images}}\n          <img src=\"{{getImage this \"productlist\" (cdn default)}}\" alt=\"{{{this.alt}}}\" data-product-image>\n        {{/each}}\n      </div>\n      {{#if product.images.length \">\" 1}}\n        <div class=\"quick-shop-secondary-image\" data-quickshop-images-secondary>\n          {{#each product.images}}\n            <div class=\"qs-img-wrapper{{#if @first}} active{{/if}}\">\n              {{> components/common/image\n                img=this\n                size=thumbnail\n                image-ratio=../theme_settings.image_ratio_grid\n                default-image=../../theme_settings.default_image\n                not-lazy=true\n              }}\n            </div>\n          {{/each}}\n        </div>\n      {{/if}}\n    {{else}}\n      {{! There is no image, show the default}}\n      {{> components/common/image\n        url=\"\"\n        size='productList'\n        default=theme_settings.default_image\n        alt=alt\n      }}\n    {{/if}}\n  </div>\n\n  <div class=\"quick-shop-info-column product-block\">\n\n    {{! Title }}\n    <h1 class=\"product-title\" data-product-title=\"{{product.title}}\">{{product.title}}</h1>\n\n    {{! Price }}\n    {{> components/product/price price=product.price schema_org=true show_savings=true}}\n\n    <a class=\"quick-shop-full-details-link\" href=\"{{product.url}}\">{{lang 'common.view_full'}}</a>\n\n    {{! Description }}\n    <div class=\"product-information-description\">\n      <div class=\"product-summary\" data-excerpt-length=\"188\">{{{product.description}}}</div>\n      <a class=\"more-link\" href=\"{{product.url}}\">{{lang 'common.more'}}</a>\n    </div>\n\n    {{! Messages }}\n    <div data-product-message></div>\n\n    {{! Add to Cart Form }}\n    {{> components/product/add-to-cart-form quickshop=true}}\n  </div>\n\n</div>\n","svg/modal-close":"<svg xmlns=\"http://www.w3.org/2000/svg\"\n  width=\"20\"\n  height=\"20\"\n  viewBox=\"0 0 20 20\"\n  aria-label=\"{{lang 'common.modals.close'}}\"\n  role=\"icon\">\n  <title>{{{lang 'common.modals.close'}}}</title>\n  <path fill-rule=\"evenodd\" fill=\"currentColor\" d=\"M8.87 10l-7.7-7.7c-.32-.32-.32-.83 0-1.14.3-.3.8-.3 1.12 0l7.7 7.7 7.7-7.7c.3-.3.8-.3 1.1 0 .3.3.3.82 0 1.13L11.1 10l7.7 7.7c.3.3.3.8 0 1.1-.3.3-.8.3-1.12 0l-7.7-7.7-7.7 7.7c-.33.3-.84.3-1.15 0-.3-.3-.3-.8 0-1.12l7.7-7.7z\"/>\n</svg>\n","components/common/image":"{{!--\n  Helper to display any image with alt text. Pass in\n  @param img         The image object\n  @param size        The preset size to use.\n  @param url         Optional: Set the url this image should link to.\n  @param default     Default image to display\n  @param image-ratio   The image class to apply to the wrapping element.\n  @param not-lazy    Optional: do not lazy load (mainly used for ajax-loaded images)\n--}}\n\n{{#if not-lazy}}\n  {{#if url}}\n    <a class=\"replaced-image {{image-ratio}} {{#unless img}}no-image{{/unless}}\"\n      href=\"{{url}}\"\n      style=\"background-image: url({{getImage img size (cdn default)}});\"\n    >\n      <img\n        src=\"{{getImage img size (cdn default)}}\"\n        alt=\"{{image.alt}}\"\n      >\n    </a>\n  {{else}}\n    <div\n      class=\"replaced-image {{image-ratio}} {{#unless img}}no-image{{/unless}}\"\n      style=\"background-image: url({{getImage img size (cdn default)}});\"\n    >\n      <img\n        src=\"{{getImage img size (cdn default)}}\"\n        alt=\"{{image.alt}}\"\n      >\n    </div>\n  {{/if}}\n{{else}}\n  {{#if url}}\n    <a class=\"lazy-image replaced-image {{image-ratio}} {{#unless img}}no-image{{/unless}}\"\n      href=\"{{url}}\"\n      data-src=\"{{getImage img size (cdn default)}}\"\n    >\n      <img\n        class=\"lazy-image\"\n        data-src=\"{{getImage img size (cdn default)}}\"\n        alt=\"{{image.alt}}\"\n      >\n    </a>\n  {{else}}\n    <div\n      class=\"lazy-image replaced-image {{image-ratio}} {{#unless img}}no-image{{/unless}}\"\n      data-src=\"{{getImage img size (cdn default)}}\"\n    >\n      <img\n        class=\"lazy-image\"\n        data-src=\"{{getImage img size (cdn default)}}\"\n        alt=\"{{image.alt}}\"\n      >\n    </div>\n  {{/if}}\n{{/if}}\n","components/product/price":"{{!\n  Product Price - Sale vs Normal, With Tax vs Without\n\n  @param this price object\n\n  @param show_savings bool (optional)\n         If true, show a label saying how much is saved from the full price.\n\n  @param schema_org bool (optional)\n         If false, will keep the schema.org meta data out of this price.\n}}\n{{#if price}}\n\n  <span class=\"price {{> components/common/price-class}}\">\n\n    {{#if price.with_tax}}\n      <span class=\"price-withtax\" data-price=\"with-tax\">\n        <span class=\"price-value\" data-price-value=\"with-tax\">\n          {{price.with_tax.formatted}}\n        </span>\n        {{#if price.rrp_with_tax}}\n          <span class=\"price-rrp\" data-price-rrp=\"with-tax\">{{price.rrp_with_tax.formatted}}</span>\n        {{/if}}\n        {{#if price.without_tax}}\n          <span class=\"price-tax-label\">{{lang 'product.including_tax' tax_label=price.tax_label}}</span>\n        {{/if}}\n      </span>\n    {{/if}}\n\n    {{#if price.without_tax}}\n      <span class=\"price-withouttax\" data-price=\"without-tax\">\n        <span class=\"price-value\" data-price-value=\"without-tax\">\n          {{price.without_tax.formatted}}\n        </span>\n        {{#if price.rrp_without_tax}}\n          <span class=\"price-rrp\" data-price-rrp=\"without-tax\">{{price.rrp_without_tax.formatted}}</span>\n        {{/if}}\n        {{#if price.with_tax}}\n          <span class=\"price-tax-label\">{{lang 'product.excluding_tax' tax_label=price.tax_label}}</span>\n        {{/if}}\n      </span>\n    {{/if}}\n\n    {{#if show_savings}}\n      {{#if price.saved}}\n        <span class=\"price-saved\" data-price-saved>\n          ({{lang 'product.you_save'}} {{price.saved.formatted}})\n        </span>\n      {{/if}}\n    {{/if}}\n\n  </span>\n{{/if}}\n","components/product/add-to-cart-form":"{{!--\n  Add To Cart Form -- As displayed within a full product page.\n--}}\n\n<form\n  class=\"form add-to-cart-form{{#unless quickshop}}-full{{/unless}}\"\n  method=\"post\"\n  action=\"{{product.cart_url}}\"\n  enctype=\"multipart/form-data\"\n  data-product-options-count=\"{{product.options.length}}\"\n  data-cart-item-add>\n  <input type=\"hidden\" name=\"action\" value=\"add\">\n  <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\" data-product-id>\n\n  <div data-product-option-change>\n    {{#if product.options}}\n      {{#each product.options}}\n        {{{dynamicComponent 'core/forms/options'}}}\n      {{/each}}\n    {{/if}}\n\n    {{#if product.customizations}}\n      {{#each product.customizations}}\n        {{{dynamicComponent 'core/forms/customizations'}}}\n      {{/each}}\n    {{/if}}\n\n    {{#if product.event_date}}\n      {{> core/products/event}}\n    {{/if}}\n\n    {{#if product.can_purchase}}\n      {{> components/common/quantity product}}\n    {{/if}}\n  </div>\n\n  <div class=\"product-cart-messages\">\n    <div data-product-cart-message>{{!-- Add to cart alerts --}}</div>\n    {{#if product.can_purchase}}\n      <div data-product-option-message>{{!-- Product option change alerts --}}</div>\n    {{/if}}\n  </div>\n\n  <div class=\"form-field add-to-cart-buttons\" data-product-add>\n\n    {{#if product.can_purchase}}\n      {{#if product.pre_order}}\n        <input class=\"button button-inverted\" type=\"submit\" value=\"{{lang 'product.pre_order'}}\" data-button-purchase data-button-preorder>\n      {{else}}\n        <input class=\"button button-inverted\" type=\"submit\" value=\"{{lang 'product.add_to_cart'}}\" data-button-purchase>\n      {{/if}}\n    {{else}}\n      {{#if product.out_of_stock}}\n        <div class=\"button button-inverted button-disabled\">\n          {{lang 'product.sold_out'}}\n        </div>\n      {{/if}}\n    {{/if}}\n\n    {{#if settings.show_wishlist}}\n      <a class=\"button button-inverted button-square\" href=\"{{product.add_to_wishlist_url}}\">{{> svg/heart}}</a>\n      <span class=\"spinner\"></span>\n    {{/if}}\n\n  </div>\n</form>\n","components/common/price-class":"{{#if price.with_tax}}\n  {{#unless price.without_tax}}\n    {{#if price.rrp_with_tax}}\n      prices-taxed-onsale\n    {{else}}\n      prices-taxed\n    {{/if}}\n  {{/unless}}\n{{/if}}\n{{#if price.without_tax}}\n  {{#unless price.with_tax}}\n    {{#if price.rrp_without_tax}}\n      prices-notax-onsale\n    {{else}}\n      prices-notax\n    {{/if}}\n  {{/unless}}\n{{/if}}\n{{#if price.with_tax}}\n  {{#if price.without_tax}}\n    {{#if price.rrp_with_tax}}\n      prices-both-onsale\n    {{else}}\n      prices-both\n    {{/if}}\n  {{/if}}\n{{/if}}\n","core/products/event":"<div class=\"form-field form-field-date form-required\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{product.event_date.name}}\n      <span class=\"required-text\">{{lang 'common.required'}}</span>\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"EventDate[Yr]\" required>\n          <option disabled selected>{{lang 'forms.date.year'}}</option>\n          {{#if product.event_date.earliest_year '==' product.event_date.latest_year}}\n            <option value=\"{{product.event_date.earliest_year}}\">{{product.event_date.earliest_year}}</option>\n          {{else}}\n            {{#for product.event_date.earliest_year product.event_date.latest_year}}\n              <option value=\"{{$index}}\">{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"EventDate[Mth]\" required>\n          <option disabled selected>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\">{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"EventDate[Day]\" required>\n          <option disabled selected>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\">{{$index}}</option>\n          {{/for}}\n        </select>\n      </span>\n    </span>\n\n  </span>\n\n  {{#if product.event_date.date_start}}\n    {{#if product.event_date.date_end}}\n      <span class=\"product-event-bounds\">{{lang 'forms.date.bounds' start_date=product.event_date.date_start end_date=product.event_date.date_end}}</span>\n    {{/if}}\n  {{/if}}\n</div>\n","components/common/quantity":"{{!\n  Quantity Widget with Increment/Decrement controls.\n\n  @param product\n         cart.item or product object\n}}\n\n<div class=\"form-field form-field-quantity-control\" data-quantity-control=\"{{id}}\">\n  <label class=\"form-label\">\n    {{#unless nolabel}}\n      <span class=\"form-field-title\">Quantity</span>\n    {{/unless}}\n    <span class=\"form-field-control\">\n      <input\n        class=\"form-input qty-amount\"\n        type=\"number\"\n        name=\"qty{{#if product_id}}-{{id}}{{else}}[]{{/if}}\"\n        pattern=\"[0-9]*\"\n        value=\"{{#if quantity}}{{quantity}}{{else}}{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}{{/if}}\"\n        min=\"{{#if min_purchase_quantity}}{{min_purchase_quantity}}{{else}}1{{/if}}\"\n        {{#if max_purchase_quantity}}max=\"{{max_purchase_quantity}}\"{{/if}}\n        data-quantity-control-input>\n      <span class=\"qty-decrement\"\n        data-quantity-control-action=\"decrement\">&ndash;</span>\n      <span class=\"qty-increment\"\n        data-quantity-control-action=\"increment\">&#43;</span></span>\n  </label>\n</div>\n","svg/heart":"<svg xmlns=\"http://www.w3.org/2000/svg\" preserveAspectRatio=\"xMidYMid\" width=\"17\" height=\"15\" viewBox=\"0 0 17 15\" aria-label=\"{{lang 'account.wishlists.heading'}}\" role=\"icon\">\n  <title>{{{lang 'account.wishlists.heading'}}}</title>\n  <path d=\"M8.496 3.263C6.296-1.94.04-.727 0 5.313-.024 8.51 8.51 14.99 8.51 14.99S17.022 8.458 17 5.277c-.04-6.054-6.404-7.01-8.504-2.014z\" id=\"path-1\" class=\"cls-2\" fill-rule=\"evenodd\"/>\n</svg>\n","core/forms/options/date":"<div class=\"form-field form-field-options form-field-date{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"date\">\n  <span class=\"form-label\">\n\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n\n    <span class=\"form-field-control\">\n      <span class=\"form-select-wrapper form-date-year\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][year]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.year'}}</option>\n          {{#if earliest_year '===' latest_year}}\n            <option value=\"{{earliest_year}}\" selected=\"selected\">{{earliest_year}}</option>\n          {{else}}\n            {{#for earliest_year latest_year}}\n              <option value=\"{{$index}}\" {{#if ../selected_date.year '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n            {{/for}}\n          {{/if}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-month\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][month]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.month'}}</option>\n          {{#for 1 12}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.month '==' $index}}selected=\"selected\"{{/if}}>{{getShortMonth $index}}</option>\n          {{/for}}\n        </select>\n      </span>\n\n      <span class=\"form-select-wrapper form-date-day\">\n        <select class=\"form-input form-select\" name=\"attribute[{{id}}][day]\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n          <option disabled>{{lang 'forms.date.day'}}</option>\n          {{#for 1 31}}\n            <option value=\"{{$index}}\" {{#if ../selected_date.day '==' $index}}selected=\"selected\"{{/if}}>{{$index}}</option>\n          {{/for}}\n        </select>\n        </span>\n    </span>\n\n  </span>\n</div>\n","core/forms/options/input-checkbox":"\n<div class=\"form-field form-field-options form-field-checkbox{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-checkbox\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    <label class=\"form-label {{class}}\">\n      <input\n        class=\"form-input form-checkbox\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"checkbox\"\n        value=\"{{value}}\"\n        {{#if checked}}checked{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n      <span class=\"form-label-text\">{{label}}</span>\n    </label>\n  </div>\n</div>\n","core/forms/options/input-file":"\n<div class=\"form-field form-field-options form-field-file {{#if required}}form-required{{/if}}\"\n  data-product-attribute=\"input-file\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <span class=\"form-file-wrapper\" data-button-label=\"{{lang 'core.account.products.upload'}}\">\n        <input\n          class=\"form-input form-file\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{id}}]\"\n          type=\"file\"\n          {{#if required}}required{{/if}}\n          aria-required=\"{{required}}\">\n      </span>\n    </span>\n  </label>\n\n  {{#if file_types '!==' ''}}\n    <span class=\"form-file-types\">{{lang 'core.account.products.filetypes' file_types=file_types}}</span>\n  {{/if}}\n\n  <span class=\"form-file-size\">{{lang 'core.account.products.max_filesize' bytes=file_size}}</span>\n</div>\n","core/forms/options/input-numbers":"\n<div class=\"form-field form-field-options form-field-number{{#if required}} form-required{{/if}}\"\n data-product-attribute=\"input-number\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\" id=\"attribute-{{id}}\" name=\"attribute[{{id}}]\" type=\"number\" value=\"{{prefill}}\" {{#if required}}required{{/if}} aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/input-text":"\n<div class=\"form-field form-field-options form-field-text{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"input-text\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{display_name}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"attribute-{{id}}\"\n        name=\"attribute[{{id}}]\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        value=\"\"\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","core/forms/options/product-list":"\n<div class=\"form-field form-field-options form-field-product-list{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"product-list\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n\n  <div class=\"form-field-control\">\n    {{#unless values.0.image}}\n      {{!-- same as radio buttons if no image set --}}\n      {{#each values}}\n        <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\"\n            value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{label}}</span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label\">\n          <input\n            class=\"form-input form-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n        </label>\n      {{/unless}}\n    {{else}}\n      {{#each values}}\n        <label class=\"form-label product-picklist-item\" data-product-attribute-value=\"{{id}}\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{../id}}]\"\n            type=\"radio\" value=\"{{id}}\"\n            {{#if selected}}checked{{/if}}\n            {{#if ../required}}required{{/if}}\n            aria-required=\"{{required}}\">\n          <span class=\"product-picklist-item-body\">\n            <span class=\"product-picklist-image-wrap\">\n              <img class=\"product-picklist-image\" src=\"{{getImage image 'core-product-picklist' (cdn ../theme_settings.default_image)}}\" alt=\"{{image.alt}}\">\n            </span>\n            <span class=\"product-picklist-label-text form-label-text\">{{label}}</span>\n          </span>\n        </label>\n      {{/each}}\n      {{#unless required}}\n        <label class=\"form-label product-picklist-item\">\n          <input\n            class=\"form-input form-radio product-picklist-radio\"\n            name=\"attribute[{{id}}]\"\n            type=\"radio\"\n            value=\"0\"\n            {{#if prefill '==' 0}}checked{{/if}}>\n          <span class=\"product-picklist-item-body picklist-none\">\n            <span class=\"product-picklist-label-text form-label-text\">{{lang 'product.none'}}</span>\n          </span>\n        </label>\n      {{/unless}}\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-radio":"\n<div class=\"form-field form-field-options form-field-radio{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-radio\">\n  <div class=\"form-field-title product-option-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-radio\"\n          id=\"attribute-{{id}}\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label\">\n        <input\n          class=\"form-input form-radio\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if prefill '==' 0}}checked{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"form-label-text\">{{lang 'core.product.none'}}</span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-rectangle":"\n<div class=\"form-field form-field-options form-field-rectangle{{#if required}} form-required{{/if}}\"\n  data-product-attribute=\"set-rectangle\">\n  <div class=\"form-field-title\">\n    {{display_name}}\n    {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n  </div>\n  <div class=\"form-field-control\">\n    {{#each values}}\n      <label class=\"form-label rectangle\" data-product-attribute-value=\"{{id}}\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          id=\"attribute-{{id}}\"\n          type=\"radio\"\n          value=\"{{id}}\"\n          {{#if selected}}checked{{/if}}\n          {{#if ../required}}required{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{label}}</span>\n      </label>\n    {{/each}}\n    {{#unless required}}\n      <label class=\"form-label rectangle\">\n        <input\n          class=\"form-input form-rectangle\"\n          name=\"attribute[{{../id}}]\"\n          type=\"radio\"\n          value=\"0\"\n          {{#if selected}}checked{{/if}}\n          aria-required=\"{{required}}\">\n        <span class=\"rectangle-text form-label-text\">{{lang 'core.product.none'}}</span>\n      </label>\n    {{/unless}}\n  </div>\n</div>\n","core/forms/options/set-select":"\n<div class=\"form-field form-field-options form-field-select{{#if req