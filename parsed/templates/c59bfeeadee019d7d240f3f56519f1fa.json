{"components/cart/shipping-quotes":"{{!--\n  Shipping Quotes - Template pulled in by shipping calculator and set up to\n  display the individual quotes that can be selected from.\n--}}\n{{!-- TODO: form elements need same class structure as rest of theme --}}\n\n<h2>{{lang 'cart.shipping_calculator.shipping_methods'}}</h2>\n\n<form class=\"form\">\n\n  <div class=\"form-field-radio\">\n    {{#each quotes.shipping_methods}}\n\n      <label class=\"form-label\" for=\"shipping-quote-{{id}}\">\n        <input\n          id=\"shipping-quote-{{id}}\"\n          class=\"form-input form-radio\"\n          type=\"radio\"\n          name=\"shipping_method\"\n          value=\"{{id}}\"\n          data-shipping-quote>\n        <span class=\"form-label-text\">{{provider_name}} {{cost.formatted}}</span>\n      </label>\n\n    {{/each}}\n\n    {{#each quotes.ups_shipping_methods}}\n      <label class=\"form-label\" for=\"shipping-quote-{{id}}\">\n        <input\n          id=\"shipping-quote-{{id}}\"\n          class=\"form-input form-radio\"\n          type=\"radio\"\n          name=\"shipping_method\"\n          value=\"{{id}}\"\n          data-shipping-quote\n        >\n        <span class=\"form-label-text\">\n          {{#if logo_path}}\n            <img src=\"{{logo_path}}\" alt=\"{{description}}\">\n          {{/if}}\n          {{description}} - {{cost.formatted}}\n        </span>\n      </label>\n    {{/each}}\n  </div>\n\n  <div class=\"form-actions shipping-quote-submit\">\n    <input class=\"button\" type=\"submit\" value=\"{{lang 'cart.shipping_calculator.select'}}\">\n    <a class=\"button button-secondary\" href=\"#\" data-modal-close>{{lang 'common.form_cancel'}}</a>\n  </div>\n</form>\n"}
