{"components/faceted-search/facets/multi":"<ul\n  class=\"facet-option {{#unless selected}}{{#if start_collapsed}}hidden{{/if}}{{/unless}}\"\n  data-count=\"{{items_count}}\"\n  data-facet-filter-wrapper>\n\n  {{#each items}}\n    <li class=\"facet-item {{#if selected}}active{{/if}}\" data-facet-item>\n      <a\n        href=\"{{url}}\"\n        rel=\"nofollow\"\n        data-faceted-search-facet>\n        {{title}}\n        {{#if ../show_product_counts}}<span data-facet-item-count>({{count}})</span>{{/if}}\n      </a>\n    </li>\n  {{/each}}\n</ul>\n\n  {{#if show_more_toggle}}\n    <a class=\"facet-list-show-more\" href=\"#\" data-facet-more=\"{{facet}}\">\n      <span class=\"toggle-link-text toggle-link-text-off\">{{lang 'faceted_search.show_more'}}</span>\n      <span class=\"toggle-link-text toggle-link-text-on\">{{lang 'faceted_search.show_less'}}</span>\n    </a>\n  {{/if}}\n"}
