{"components/product/images-list":"{{!--\n  Images List - Secondary images displayed as thumbnails\n  Used to control the primary image displayed.\n\n  @param size - the size of the images to load\n--}}\n\n{{inject 'imgClass' theme_settings.image-ratio-thumbnail}}\n\n<ul class=\"product-images-pagination\" data-images-pagination>\n  {{#each product.images}}\n    <li>\n      <a\n        class=\"replaced-image {{../theme_settings.image-ratio-thumbnail}}\"\n        href=\"{{getImage this 'product'}}\"\n        style=\"background-image: url({{getImage this ../size}})\">\n        <img class=\"visually-hidden\" src=\"{{getImage this ../size}}\" alt=\"{{alt}}\">\n      </a>\n    </li>\n  {{/each}}\n  {{#each product.videos.list}}\n    <li class=\"product-images-pagination-video\">\n      <a\n        class=\"replaced-image {{../theme_settings.image-ratio-thumbnail}}\"\n        href=\"https://www.youtube.com/watch?v={{id}}\"\n        target=\"_blank\"\n        style=\"background-image: url(https://img.youtube.com/vi/{{id}}/0.jpg)\">\n        <img class=\"visually-hidden\"\n          src=\"https://img.youtube.com/vi/{{id}}/0.jpg\"\n          alt=\"{{'core.account.products.videos.header'}}\">\n      </a>\n    </li>\n  {{/each}}\n</ul>\n"}
