{"components/cart/shipping-calculator":"{{!-- TODO: form elements need same class structure as rest of theme --}}\n<div class=\"shipping-calculator\" data-shipping-calculator>\n  <a href=\"#\" class=\"modal-close\">{{> svg/modal-close}}</a>\n\n  <h2 class=\"section-title\">{{lang 'cart.shipping_calculator.title'}}</h2>\n  <div data-shipping-errors>{{!-- Shipping Messages --}}</div>\n\n  {{!-- Select Destination Form --}}\n  <form class=\"form\">\n    <div class=\"form-field form-field-select form-required\">\n      <label class=\"form-label\" for=\"shipping-country\">\n        <span class=\"form-label-text\">\n          {{lang 'cart.shipping_calculator.select_a_country'}}\n          <span class=\"required-text\">{{lang 'common.required'}}</span>\n        </span>\n        <span class=\"form-field-control\">\n          <span class=\"form-select-wrapper\">\n            <select class=\"form-select\" id=\"shipping-country\" name=\"shipping-country\" data-field-type=\"Country\">\n              <option>{{lang 'cart.shipping_calculator.select_a_country'}}</option>\n              {{#each countries}}\n                <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                  {{name}}\n                </option>\n              {{/each}}\n            </select>\n          </span>\n        </span>\n      </label>\n    </div>\n\n    <div class=\"form-field form-field-select form-required\">\n      <label class=\"form-label\" for=\"shipping-state\">\n        <span class=\"form-field-title\">\n          {{lang 'cart.shipping_calculator.select_a_state'}}\n          <span class=\"required-text\">{{lang 'common.required'}}</span>\n        </span>\n        <span class=\"form-field-control\">\n          <span class=\"form-select-wrapper\">\n            <select class=\"form-select form-input\" id=\"shipping-state\" name=\"shipping-state\" data-field-type=\"State\">\n              <option>{{lang 'cart.shipping_calculator.select_a_state'}}</option>\n              {{#each states}}\n                <option value=\"{{id}}\" {{#if selected}}selected=\"selected\"{{/if}}>\n                  {{name}}\n                </option>\n              {{/each}}\n            </select>\n          </span>\n        </span>\n      </label>\n    </div>\n\n    {{> core/forms/common/text\n      label=(lang 'cart.shipping_calculator.zip_postal_code')\n      name=\"shipping-zip\"\n      id=\"shipping-zip\"\n    }}\n\n    <div class=\"form-actions\">\n      <button class=\"button\">{{lang 'cart.shipping_calculator.estimate_shipping'}}</button>\n    </div>\n  </form>\n\n  <div data-shipping-quotes class=\"shipping-quotes\">{{!-- API Returned Quotes Display Here--}}</div>\n</div>\n","core/forms/common/text":"\n<div class=\"form-field form-field-common form-field-text {{#if required}} form-required{{/if}}\" id=\"{{id}}\">\n  <label class=\"form-label\">\n    <span class=\"form-field-title\">\n      {{label}}\n      {{#if required}}<span class=\"required-text\">{{lang 'common.required'}}</span>{{/if}}\n    </span>\n    <span class=\"form-field-control\">\n      <input class=\"form-input\"\n        id=\"input-{{id}}\"\n        name=\"{{name}}\"\n        type=\"text\"\n        placeholder=\"{{prefill}}\"\n        {{#if value}}value=\"{{value}}\"{{/if}}\n        {{#if private_id}}data-field-type=\"{{private_id}}\"{{/if}}\n        {{#if required}}required{{/if}}\n        aria-required=\"{{required}}\">\n    </span>\n  </label>\n</div>\n","svg/modal-close":"<svg xmlns=\"http://www.w3.org/2000/svg\"\n  width=\"20\"\n  height=\"20\"\n  viewBox=\"0 0 20 20\"\n  aria-label=\"{{lang 'common.modals.close'}}\"\n  role=\"icon\">\n  <title>{{{lang 'common.modals.close'}}}</title>\n  <path fill-rule=\"evenodd\" fill=\"currentColor\" d=\"M8.87 10l-7.7-7.7c-.32-.32-.32-.83 0-1.14.3-.3.8-.3 1.12 0l7.7 7.7 7.7-7.7c.3-.3.8-.3 1.1 0 .3.3.3.82 0 1.13L11.1 10l7.7 7.7c.3.3.3.8 0 1.1-.3.3-.8.3-1.12 0l-7.7-7.7-7.7 7.7c-.33.3-.84.3-1.15 0-.3-.3-.3-.8 0-1.12l7.7-7.7z\"/>\n</svg>\n"}
