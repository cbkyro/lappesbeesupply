{"components/common/newsletter-subscribe":"<form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\">\n  <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n  <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n  <input type=\"hidden\" name=\"check\" value=\"1\">\n  <fieldset class=\"newsletter-subscribe\">\n    <label class=\"visually-hidden\" for=\"nl_email\">{{lang 'common.newsletter.email_placeholder'}}</label>\n    <div class=\"input-wrap-outer\">\n    <input class=\"button\" type=\"submit\" value=\"{{lang 'common.newsletter.submit'}}\">\n      <div class=\"input-wrap\">\n        <input\n          class=\"form-input\"\n          id=\"nl_email\"\n          name=\"nl_email\"\n          type=\"email\"\n          value=\"{{customer.email}}\"\n          required\n          placeholder=\"{{lang 'common.newsletter.email_placeholder'}}\">\n      </div>\n    </div>\n  </fieldset>\n</form>\n"}
