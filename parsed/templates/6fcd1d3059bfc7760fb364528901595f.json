{"components/product/images":"{{!--\n  Product Media\n  The main full size images and videos displayed on product pages.\n\n  @param size - the size of the images to load\n--}}\n\n<div class=\"product-slides-container\" id=\"product-images\">\n  <div class=\"product-slides-wrap\" data-images-primary>\n    {{#if product.images}}\n      {{#each product.images}}\n        <a class=\"product-slide\" href=\"{{getImage this}}\" target=\"_blank\">\n          <img\n            src=\"{{getImage this ../size}}\"\n            alt=\"{{alt}}\"\n            title=\"{{alt}}\"\n          >\n        </a>\n      {{/each}}\n    {{else}}\n      <a class=\"product-slide\" href=\"{{getImage this}}\" target=\"_blank\">\n        <img src=\"{{getImage theme_settings.default_image ../size}}\" alt=\"{{alt}}\">\n      </a>\n    {{/if}}\n    {{#each product.videos.list}}\n      <div class=\"product-slide product-slide-video\">\n        <iframe\n          width=\"480\"\n          height=\"315\"\n          src=\"https://www.youtube.com/embed/{{id}}?rel=0&showinfo=0&autohide=1\"\n          frameborder=\"0\"\n          allowfullscreen\n        ></iframe>\n      </div>\n    {{/each}}\n  </div>\n</div>\n"}